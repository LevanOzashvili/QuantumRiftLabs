{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "env": {
      "node": true,
      "jest": true
    }
}
